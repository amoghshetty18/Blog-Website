<%- include("partials/header"); -%>
<h1>Home</h1>
<p><%= startingContent %></p>

<% posts.forEach(function(post){ %>

<h1><%=post.title%></h1>
<p>
  <%=post.content.substring(0, 300) + " ..."%>
  <a href="/posts/<%=post._id%>">Read More</a>
</p>

<% }) %>

<a href="compose" class="mt-3 btn btn-default compose">Compose</a>

<%- include("partials/footer"); -%>
